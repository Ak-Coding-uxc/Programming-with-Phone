#loops -> while / for
# for loop
lists = ['a','b','c','d']
k = len(lists)
print(k)
print(type(k))
print(f'k is {type(k)}')
i = 0
while(i<k):
    print(lists[i])
    i += 1
print('\n\n')
for list in lists:
    print(list)

#print('\n\n\n')
l = range(0,25,2)
my_list = list(l)
for m in my_list:
    print(m) 